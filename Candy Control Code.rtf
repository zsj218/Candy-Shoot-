music.setVolume(100)
music.playSound(music.sounds(Sounds.PowerUp))
pins.A1.servoWrite(30)
input.pinA1.setThreshold(200)
forever(function () {
    if (input.pinA3.isPressed()) {
        pins.A1.servoWrite(180)
        music.playSound(music.sounds(Sounds.PowerUp))
    } else {
        light.clear()
    }
})

